# secrets.yaml.example
# Copy this file to secrets.yaml and fill in your actual credentials
# NEVER commit secrets.yaml to version control!

# WiFi credentials
wifi_ssid: "Your_WiFi_SSID"
wifi_password: "Your_WiFi_Password"

# Optional: Fallback WiFi credentials
# wifi_ssid_2: "Backup_WiFi_SSID"
# wifi_password_2: "Backup_WiFi_Password"

# Optional: Static IP configuration (if needed)
# static_ip: "192.168.1.100"
# gateway: "192.168.1.1"
# subnet: "255.255.255.0"

# Optional: Home Assistant configuration (usually auto-discovered)
# ha_url: "http://192.168.1.50:8123"
# ha_token: "your_long_lived_access_token_here"
